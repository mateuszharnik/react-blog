{
  "name": "react-blog",
  "version": "1.0.0",
  "author": "Mateusz Harnik",
  "license": "MIT",
  "main": "./src/server/index.js",
  "private": true,
  "scripts": {
    "restart": "rimraf dist && rimraf yarn.lock && rimraf node_modules",
    "clean": "rimraf dist",
    "deploy": "git push heroku master",
    "start": "yarn run migration:up:prod && cross-env NODE_ENV=production node ./dist/server/server.js",
    "start:client": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.client.js --mode=development",
    "start:server": "yarn run migration:up:dev && cross-env NODE_ENV=development nodemon --exec babel-node ./src/server",
    "start:dev": "concurrently -n \"@backend,@frontend\" -c \"blue,red\" \"yarn run start:server\" \"yarn run start:client\"",
    "start:e2e:client": "cross-env NODE_ENV=test webpack-dev-server --config ./webpack/webpack.config.client.js --mode=development",
    "start:e2e:server": "yarn run migration:up:test && cross-env NODE_ENV=test nodemon --exec babel-node ./src/server",
    "start:e2e": "concurrently -n \"@backend,@frontend,@e2e\" -c \"blue,red,green\" \"yarn run start:e2e:server\" \"yarn run start:e2e:client\" \"yarn run test:e2e:open\"",
    "start:test": "concurrently -n \"@backend,@frontend\" -c \"blue,red\" \"yarn run start:e2e:server\" \"yarn run start:e2e:client\"",
    "build": "yarn run build:client && yarn run purgecss && yarn run build:server",
    "build:analyze": "yarn run build:client:analyze && yarn run build:server",
    "build:client": "cross-env NODE_ENV=production webpack -p --mode=production --config ./webpack/webpack.config.client.js",
    "build:client:analyze": "cross-env NODE_ENV=production ANALYZE=true webpack -p --mode=production --config ./webpack/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack -p --mode=production --config ./webpack/webpack.config.server.js",
    "test": "yarn run test:client && yarn run test:server && yarn run test:e2e",
    "test:client": "cross-env NODE_ENV=test jest ./src/client --passWithNoTests",
    "test:server": "cross-env NODE_ENV=test jest ./src/server --env=node --passWithNoTests",
    "test:e2e": "cross-env NODE_ENV=test cypress run",
    "test:e2e:open": "cross-env NODE_ENV=test cypress open",
    "purgecss": "purgecss --css dist/client/css/styles.css --content dist/client/index.html dist/client/js/**/*.js --output dist/client/css/styles.css",
    "lint-fix": "yarn run lint:js-fix && yarn run lint:e2e-fix && yarn run lint:style-fix",
    "lint:js-fix": "eslint ./src/ --ext .js,.jsx --fix",
    "lint:server-fix": "eslint ./src/server/ --ext .js --fix",
    "lint:client-fix": "eslint ./src/client/ --ext .js,.jsx --fix",
    "lint:e2e-fix": "eslint ./src/e2e/ --ext .js --fix",
    "lint:style-fix": "stylelint ./src/client/**/*.{css,scss} --fix",
    "lint:eslint-fix": "eslint .eslintrc.js --no-ignore .eslintrc.js --fix",
    "lint": "yarn run lint:js && yarn run lint:e2e && yarn run lint:style",
    "lint:js": "eslint ./src/ --ext .js,.jsx",
    "lint:server": "eslint ./src/server/ --ext .js",
    "lint:client": "eslint ./src/client/ --ext .js,.jsx",
    "lint:e2e": "eslint ./src/e2e/ --ext .js",
    "lint:style": "stylelint ./src/client/**/*.{css,scss}",
    "lint:eslint": "eslint .eslintrc.js --no-ignore .eslintrc.js",
    "seed": "cross-env NODE_ENV=development babel-node ./src/server/seeds/index.js",
    "clean-db:prod": "cross-env NODE_ENV=production babel-node ./src/server/seeds/clean.js",
    "clean-db:dev": "cross-env NODE_ENV=development babel-node ./src/server/seeds/clean.js",
    "clean-db:test": "cross-env NODE_ENV=test babel-node ./src/server/seeds/clean.js",
    "migration:create": "npx migrate-mongo create -f migrations.config.js",
    "migration:up:prod": "cross-env NODE_ENV=production npx migrate-mongo up -f migrations.config.js",
    "migration:down:prod": "cross-env NODE_ENV=production npx migrate-mongo down -f migrations.config.js",
    "migration:status:prod": "cross-env NODE_ENV=production npx migrate-mongo status -f migrations.config.js",
    "migration:up:dev": "cross-env NODE_ENV=development npx migrate-mongo up -f migrations.config.js",
    "migration:down:dev": "cross-env NODE_ENV=development npx migrate-mongo down -f migrations.config.js",
    "migration:status:dev": "cross-env NODE_ENV=development npx migrate-mongo status -f migrations.config.js",
    "migration:up:test": "cross-env NODE_ENV=test npx migrate-mongo up -f migrations.config.js",
    "migration:down:test": "cross-env NODE_ENV=test npx migrate-mongo down -f migrations.config.js",
    "migration:status:test": "cross-env NODE_ENV=test npx migrate-mongo status -f migrations.config.js"
  },
  "lint-staged": {
    "src/server/**/*.js": [
      "yarn run lint:server"
    ],
    "src/server/**/*.test.js": [
      "yarn run test:server"
    ],
    "src/client/**/*.{js,jsx}": [
      "yarn run lint:client"
    ],
    "src/client/**/*.test.js": [
      "yarn run test:client"
    ],
    "src/client/**/*.{scss,css}": [
      "yarn run lint:style"
    ],
    "src/e2e/**/*.js": [
      "yarn run lint:e2e"
    ]
  },
  "engines": {
    "node": "14.15.0",
    "npm": "6.14.8",
    "yarn": "1.22.17"
  },
  "dependencies": {
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.16.5",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "aos": "^2.3.4",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.1.3",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dompurify": "^2.3.5",
    "dotenv": "^10.0.0",
    "easy-peasy": "^5.0.4",
    "easymde": "^2.16.1",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "formik": "^2.2.9",
    "he": "^1.2.0",
    "helmet": "^4.6.0",
    "highlight.js": "^11.4.0",
    "joi": "^17.5.0",
    "jsdom": "^19.0.0",
    "jsonwebtoken": "^8.5.1",
    "jump.js": "^1.0.2",
    "jwt-decode": "^3.1.2",
    "lazysizes": "^5.3.2",
    "lodash": "^4.17.21",
    "marked": "^4.0.12",
    "migrate-mongo": "^9.0.0",
    "mongoose": "^6.1.1",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "nodemailer": "^6.7.2",
    "overlayscrollbars": "^1.13.1",
    "overlayscrollbars-react": "^0.2.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.1.1",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "browserslist": "^4.17.1",
    "clean-css": "^5.1.5",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.2.1",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.2.7",
    "cypress": "^9.5.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.15",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.4.1",
    "purgecss": "^4.0.3",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-webpack-plugin": "^2.2.2",
    "supertest": "^6.1.6",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-fix-style-only-entries": "^0.6.1",
    "webpack-node-externals": "^3.0.0"
  }
}
