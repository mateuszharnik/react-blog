{
  "name": "react-blog",
  "version": "0.3.0",
  "author": "Mateusz Harnik",
  "license": "MIT",
  "main": "./src/server/index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf ./docker/docker-data && rimraf ./storybook-static && rimraf dist && rimraf coverage && rimraf yarn.lock && rimraf node_modules",
    "start": "cross-env NODE_ENV=production node ./dist/server/server.js",
    "build": "yarn run build:client && yarn run build:server && yarn run purgecss",
    "build:client": "cross-env NODE_ENV=production webpack -p --mode=production --config ./webpack/webpack.config.client.js && rimraf dist/client/js/*.js.map",
    "build:server": "cross-env NODE_ENV=production webpack -p --mode=production --config ./webpack/webpack.config.server.js",
    "dev": "concurrently -n \"@frontend,@backend\" -c \"blue,red\" \"yarn run dev:client\" \"yarn run dev:server\"",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.client.js --mode=development",
    "dev:server": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/server",
    "test": "yarn run test:client && yarn run test:server && yarn run test:shared",
    "test:ci": "yarn run test:client --ci && yarn run test:server --ci && yarn run test:shared --ci",
    "test:client": "cross-env NODE_ENV=test APP_ENV=test COVERAGE_DIR=client jest ./src/client --runInBand --coverage",
    "test:server": "cross-env NODE_ENV=test APP_ENV=test COVERAGE_DIR=server jest ./src/server --runInBand --coverage --env=node",
    "test:shared": "cross-env NODE_ENV=test APP_ENV=test COVERAGE_DIR=shared jest ./src/shared --runInBand --coverage",
    "e2e:chrome": "cross-env NODE_ENV=test APP_ENV=e2e cypress run --browser chrome",
    "e2e:open": "cross-env NODE_ENV=test APP_ENV=e2e cypress open",
    "purgecss": "purgecss --css dist/client/css/styles.css --content dist/client/index.html dist/client/js/**/*.js --output dist/client/css/styles.css",
    "lint": "yarn run lint:js && yarn run lint:style",
    "lint:js": "cross-env NODE_ENV=production eslint ./src/ --ext .js,.jsx,.json",
    "lint:server": "cross-env NODE_ENV=production eslint ./src/server/ --ext .js,.json",
    "lint:client": "cross-env NODE_ENV=production eslint ./src/client/ --ext .js,.jsx,.json",
    "lint:shared": "cross-env NODE_ENV=production eslint ./src/shared/ --ext .js",
    "lint:e2e": "cross-env NODE_ENV=production eslint ./src/e2e/ --ext .js,.json",
    "lint:style": "stylelint ./src/client/**/*.{css,scss}",
    "migration:create": "migrate-mongo create -f migrations.config.js",
    "migration:up": "migrate-mongo up -f migrations.config.js",
    "migration:down": "migrate-mongo down -f migrations.config.js",
    "migration:status": "migrate-mongo status -f migrations.config.js"
  },
  "lint-staged": {
    "src/server/**/*.{js,json}": [
      "yarn run lint:server"
    ],
    "src/server/**/*.test.js": [
      "yarn run test:server"
    ],
    "src/client/**/*.{js,jsx,json}": [
      "yarn run lint:client"
    ],
    "src/client/**/*.test.js": [
      "yarn run test:client"
    ],
    "src/client/**/*.{scss,css}": [
      "yarn run lint:style"
    ],
    "src/shared/**/*.js": [
      "yarn run lint:shared"
    ],
    "src/shared/**/*.test.js": [
      "yarn run test:shared"
    ],
    "src/e2e/**/*.{js,json}": [
      "yarn run lint:e2e"
    ]
  },
  "engines": {
    "node": "16.19.0",
    "npm": "8.19.3"
  },
