{
  "name": "react-blog",
  "version": "1.0.0",
  "author": "Mateusz Harnik",
  "license": "MIT",
  "main": "./src/server/index.js",
  "private": true,
  "scripts": {
    "restart": "rimraf ./docker/docker-data && rimraf dist && rimraf coverage && rimraf yarn.lock && rimraf node_modules",
    "clean": "rimraf dist",
    "deploy": "git push heroku master",
    "start": "cross-env APP_ENV=production node ./dist/server/server.js",
    "start:staging": "cross-env APP_ENV=staging node ./dist/server/server.js",
    "start:testing": "cross-env APP_ENV=testing node ./dist/server/server.js",
    "start:e2e": "cross-env NODE_ENV=production APP_ENV=e2e node ./dist/server/server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.client.js --mode=development",
    "dev:server": "cross-env NODE_ENV=development APP_ENV=development nodemon --exec babel-node ./src/server",
    "dev:server:e2e": "cross-env NODE_ENV=development APP_ENV=e2e nodemon --exec babel-node ./src/server",
    "dev:server:test": "cross-env NODE_ENV=development APP_ENV=test nodemon --exec babel-node ./src/server",
    "dev": "concurrently -n \"@frontend,@backend\" -c \"blue,red\" \"yarn run dev:client\" \"yarn run dev:server\"",
    "dev:test": "concurrently -n \"@frontend,@backend\" -c \"blue,red\" \"yarn run dev:client\" \"yarn run dev:server:test\"",
    "dev:e2e": "concurrently -n \"@frontend,@backend\" -c \"blue,red\" \"yarn run dev:client\" \"yarn run dev:server:e2e\"",
    "build": "yarn run build:client && yarn run purgecss && yarn run build:server",
    "build:client": "cross-env NODE_ENV=production webpack -p --mode=production --config ./webpack/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack -p --mode=production --config ./webpack/webpack.config.server.js",
    "test": "yarn run test:client && yarn run test:server && yarn run test:shared",
    "test:client": "cross-env NODE_ENV=test APP_ENV=test COVERAGE_DIR=client jest ./src/client --runInBand --coverage",
    "test:server": "cross-env NODE_ENV=test APP_ENV=test COVERAGE_DIR=server jest ./src/server --runInBand --coverage --env=node",
    "test:shared": "cross-env NODE_ENV=test APP_ENV=test COVERAGE_DIR=shared jest ./src/shared --runInBand --coverage",
    "e2e:chrome": "cross-env NODE_ENV=test APP_ENV=e2e cypress run --browser chrome",
    "e2e:open": "cross-env NODE_ENV=test APP_ENV=e2e cypress open",
    "purgecss": "purgecss --css dist/client/css/styles.css --content dist/client/index.html dist/client/js/**/*.js --output dist/client/css/styles.css",
    "lint": "yarn run lint:js && yarn run lint:style",
    "lint:js": "cross-env NODE_ENV=production eslint ./src/ --ext .js,.jsx",
    "lint:server": "cross-env NODE_ENV=production eslint ./src/server/ --ext .js",
    "lint:client": "cross-env NODE_ENV=production eslint ./src/client/ --ext .js,.jsx",
    "lint:shared": "cross-env NODE_ENV=production eslint ./src/shared/ --ext .js",
    "lint:e2e": "cross-env NODE_ENV=production eslint ./src/e2e/ --ext .js",
    "lint:style": "stylelint ./src/client/**/*.{css,scss}",
    "migration:up": "migrate-mongo up -f migrations.config.js",
    "heroku-postbuild": "yarn run migration:up && yarn run build"
  },
  "lint-staged": {
    "src/server/**/*.js": [
      "yarn run lint:server"
    ],
    "src/server/**/*.test.js": [
      "yarn run test:server"
    ],
    "src/client/**/*.{js,jsx}": [
      "yarn run lint:client"
    ],
    "src/client/**/*.test.js": [
      "yarn run test:client"
    ],
    "src/client/**/*.{scss,css}": [
      "yarn run lint:style"
    ],
    "src/shared/**/*.js": [
      "yarn run lint:shared"
    ],
    "src/shared/**/*.test.js": [
      "yarn run test:shared"
    ],
    "src/e2e/**/*.js": [
      "yarn run lint:e2e"
    ]
  },
  "engines": {
    "node": "14.15.0",
    "npm": "6.14.8"
  },
  "dependencies": {
    "@babel/register": "~7.18.9",
    "@babel/runtime": "~7.18.9",
    "@fortawesome/fontawesome-free": "~5.15.4",
    "@fortawesome/fontawesome-svg-core": "~1.2.36",
    "@fortawesome/free-brands-svg-icons": "~5.15.4",
    "@fortawesome/free-regular-svg-icons": "~5.15.4",
    "@fortawesome/free-solid-svg-icons": "~5.15.4",
    "@fortawesome/react-fontawesome": "~0.2.0",
    "aos": "~2.3.4",
    "axios": "~0.27.2",
    "bcryptjs": "~2.4.3",
    "bootstrap": "~5.2.0",
    "colors": "~1.4.0",
    "compression": "~1.7.4",
    "cookie-parser": "~1.4.6",
    "cors": "~2.8.5",
    "cross-env": "~7.0.3",
    "csurf": "~1.11.0",
    "dompurify": "~2.3.10",
    "dotenv": "~10.0.0",
    "easy-peasy": "~5.0.4",
    "easymde": "~2.16.1",
    "express": "~4.18.1",
    "express-rate-limit": "~5.5.1",
    "formik": "~2.2.9",
    "he": "~1.2.0",
    "helmet": "~4.6.0",
    "highlight.js": "~11.6.0",
    "joi": "~17.6.0",
    "jsdom": "~19.0.0",
    "jsonwebtoken": "~8.5.1",
    "jump.js": "~1.0.2",
    "jwt-decode": "~3.1.2",
    "lazysizes": "~5.3.2",
    "lodash": "~4.17.21",
    "loglevel": "~1.8.0",
    "marked": "~4.0.18",
    "migrate-mongo": "~9.0.0",
    "mongoose": "~6.5.0",
    "morgan": "~1.10.0",
    "ms": "~2.1.3",
    "nodemailer": "~6.7.7",
    "overlayscrollbars": "~1.13.3",
    "overlayscrollbars-react": "~0.2.3",
    "prop-types": "~15.8.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-loadable": "~5.5.0",
    "react-redux": "~7.2.8",
    "react-router-dom": "~6.3.0",
    "react-transition-group": "~4.4.4",
    "redux": "~4.2.0",
    "redux-devtools-extension": "~2.13.9",
    "redux-thunk": "~2.4.1",
    "swagger-ui-express": "~4.5.0",
    "uuid": "~8.3.2",
    "yup": "~0.32.11"
  },
  "devDependencies": {
    "@babel/core": "~7.18.9",
    "@babel/eslint-parser": "~7.18.9",
    "@babel/node": "~7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.18.6",
    "@babel/plugin-proposal-optional-chaining": "~7.18.9",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/plugin-transform-runtime": "~7.18.9",
    "@babel/preset-env": "~7.18.9",
    "@babel/preset-react": "~7.18.6",
    "@cypress/browserify-preprocessor": "~3.0.2",
    "@testing-library/jest-dom": "~5.16.4",
    "@testing-library/react": "~11.2.7",
    "@vue/preload-webpack-plugin": "~2.0.0",
    "autoprefixer": "~9.8.8",
    "babel-jest": "~26.6.3",
    "babel-loader": "~8.2.5",
    "babel-plugin-webpack-alias": "~2.1.2",
    "browserslist": "~4.21.3",
    "clean-css": "~5.3.1",
    "clean-webpack-plugin": "~4.0.0",
    "concurrently": "~6.5.1",
    "copy-webpack-plugin": "~6.4.1",
    "css-loader": "~5.2.7",
    "cypress": "~9.7.0",
    "eslint": "~7.32.0",
    "eslint-config-airbnb": "~18.2.1",
    "eslint-import-resolver-webpack": "~0.13.2",
    "eslint-plugin-cypress": "~2.12.1",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jsx-a11y": "~6.6.1",
    "eslint-plugin-react": "~7.26.1",
    "eslint-plugin-react-hooks": "~4.2.0",
    "eslint-webpack-plugin": "~2.7.0",
    "file-loader": "~6.2.0",
    "friendly-errors-webpack-plugin": "~1.7.0",
    "html-webpack-plugin": "~4.5.2",
    "husky": "~4.3.8",
    "imagemin-webpack-plugin": "~2.4.2",
    "jest": "~26.6.3",
    "lint-staged": "~11.2.6",
    "mini-css-extract-plugin": "~1.6.2",
    "node-sass": "~6.0.1",
    "nodemon": "~2.0.19",
    "optimize-css-assets-webpack-plugin": "~6.0.1",
    "postcss-loader": "~3.0.0",
    "prettier": "~2.7.1",
    "purgecss": "~4.1.3",
    "rimraf": "~3.0.2",
    "sass-loader": "~10.3.1",
    "style-loader": "~2.0.0",
    "stylelint": "~13.13.1",
    "stylelint-order": "~4.1.0",
    "stylelint-prettier": "~1.2.0",
    "stylelint-webpack-plugin": "~2.4.0",
    "supertest": "~6.2.4",
    "url-loader": "~4.1.1",
    "webpack": "~4.46.0",
    "webpack-bundle-analyzer": "~3.9.0",
    "webpack-cli": "~3.3.12",
    "webpack-dev-server": "~3.11.3",
    "webpack-fix-style-only-entries": "~0.6.1",
    "webpack-node-externals": "~3.0.0"
  }
}
